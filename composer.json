{
    "name": "projectmyst/roundccube-automatic_addressbook",
    "type": "roundcube-plugin",
    "description": "The automatic addressbook plugin collect each address you send an email to and records it in an address book, making it available for later use or auto-completion.",
    "keywords": ["addressbook","automatic"],
    "homepage": "https://github.com/projectmyst/roundcube-automatic_addressbook",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Geraldo V Vieira",
            "email": "admin@hostoncilla.co.uk",
            "role": "Lead"
        },
        {
            "name": "Jocelyn Delalande",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://plugins.roundcube.net"
        }
    ],
    "require":
    {
        "php": ">=5.3.0",
        "roundcube/plugin-installer": ">=0.1.6"
    },
    "extra":
    {
        "roundcube": {
            "min-version": "0.6.0",
            "max-version": "10.21",
            "sql-dir": "SQL"
        }
    } 
}
